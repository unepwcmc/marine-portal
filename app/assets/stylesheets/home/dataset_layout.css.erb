div#outer_layout {
	background: #F8F7F4;
}

div#layout {
	overflow: hidden;
	width: 945px;
	margin-bottom: 60px;
}

div#data_set_title {
	overflow: hidden;
	width: 945px;
}

div#data_set_title ul li {
	float: left;
}


div#data_set_title ul li.gray {
	width: 200px;
	padding:20px 0 20px 20px;
	font:bold 21px ;
	color:white;
	background:#666;
}

div#data_set_title ul li.information.two_lines {
	width: 545px;
	background:#ececec;
	font:bold 24px ;
	color:#999999;
	padding:20px 0 20px 20px;
}

div#data_set_content {
	width: 945px;
}

div.content_left {
	width: 745px;
	overflow: hidden;
	float: left;
	background: #fff;
}

div.content_left ul {
	width: 705px;
	margin: 0 20px;
	padding: 20px 0;
	border-bottom: 1px solid #ddd;
	overflow: hidden;
}

div.content_left ul:last-child {
	padding-bottom: 40px;
	border-bottom: none;
}

div#data_set_content div.content_right {
	width: 200px;
}

div#data_set_content div.content_left ul li.detail {
	padding: 0 20px 0 0;
	float: left;
	width: 180px;
}

div#data_set_content div.content_left ul li.information {
	float: right;
	padding: 0 0 0 20px;
	width: 525px;
}

div#data_set_content div.content_left ul li.information p {
	margin-bottom: 10px;
}

div#data_set_content div.content_left ul li.information p:last-child {
	margin-bottom: 0;
}

div#data_set_content div.content_right {
    float: right;
    width: 200px;
}

div#data_set_content div.content_right div.data_set_box {
    background: rgba(0,0,0,.1);
    width: 200px;
    overflow: hidden;
}

div#data_set_content div.content_right div.data_set_box div.buttons {
    border-bottom: 1px solid #BABABA;
    padding: 20px 0;
    width: 185px;
    margin: auto;
}

div#data_set_content div.content_right div.data_set_box div.buttons a {
	display: block;
}

div#data_set_content div.content_right div.data_set_box div.last_date {
    width: 185px;
    border-top: 1px solid #E3E3E3;
    margin: auto;
    padding: 15px 0;
}

div#data_set_content div.content_right div.data_set_box h4 {
    font: bold 11px;
    color: #666666;
    text-transform: uppercase;
    margin: 0 0 10px 0;
}

div#data_set_content div.content_right div.data_set_box p.date {
    font: 27px 'MuseoSans-500', 'Arial', sans-serif;
    color: #666666;
}

div#data_set_content div.content_left ul li.information ul {
    width: 100%;
    border-bottom: none;
    background: none;
    width: 525px;
    margin: 0;
    padding: 0;
}

div#data_set_content div.content_left ul li.information ul li {
    float: left;
    width: 100%;
    font: normal 13px;
    color: #333333;
    padding-bottom: 10px;
}

div#data_set_content div.content_left ul li.information ul li:last-child {
	padding-bottom:0;
}

div#data_set_content div.content_left ul li.information ul li a, div#data_set_content div.content_left ul li.information p a {
    text-decoration: none;
    color: #4295D5;
    border-bottom: 1px solid #4295D5;
}

div#data_set_content div.content_left ul li.information ul li a:hover, div#data_set_content div.content_left ul li.information p a:hover {
    color: #377DB4;
    border-bottom: 1px solid #377DB4;
}



